{
  "functions": {
    "api/index.py": {
      "runtime": "python3.11",
      "maxDuration": 10,
      "memory": 2048
    }
  },
  "routes": [
    { "src": "/ws/producer", "dest": "/api/index.py" },
    { "src": "/ws/viewer", "dest": "/api/index.py" },
    { "src": "/(.*)", "dest": "/api/index.py" }
  ]
}
